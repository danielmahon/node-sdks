{
  "name": "@livekit/rtc-node",
  "description": "LiveKit RTC Node",
  "license": "Apache-2.0",
  "author": "LiveKit",
  "version": "0.4.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/livekit/node-sdks.git",
    "directory": "packages/livekit-rtc"
  },
  "files": [
    "dist",
    "src",
    "Cargo.toml",
    "Cargo.lock"
  ],
  "dependencies": {
    "@bufbuild/protobuf": "^1.4.2",
    "typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@neon-rs/cli": "0.1.73",
    "@types/node": "^20.9.2",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "build:tsc": "tsc && cp -r src/napi dist/",
    "lint": "eslint -f unix \"src/**/*.ts\" --ignore-pattern \"src/proto/*\"",
    "cargo-build": "cargo build --message-format=json > cargo.log",
    "postcargo-build": "neon dist < cargo.log",
    "debug": "npm run cargo-build --",
    "build": "pnpm run build:tsc && npm run cargo-build -- --release"
  }
}
